import cv2
video = cv2.VideoCapture("video.mp4")
frames = []
while True:
    ret, frame = video.read()
    if not ret:
        break
    frames.append(frame)
for frame in reversed(frames):
    cv2.imshow("Reverse Video", frame)
    if cv2.waitKey(30) & 0xFF == 27:
        break
cv2.destroyAllWindows()
